        {% endif %}
      });
        
      jQuery(document.body).on('afterGetCart.ajaxCart', function(evt, cart) {
        timber.cartUpdatePopup(cart);
        timber.geoIP();
      });  
        
      ShopifyAPI.getCart("");
      
      {% if settings.cart_timer_enable and template == 'cart' %}
